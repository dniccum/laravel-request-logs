{
    "name": "dniccum/laravel-request-logs",
    "description": "Log and debug HTTP requests within your Laravel application.",
    "keywords": [
        "dniccum",
        "laravel",
        "request-logs",
        "logs",
        "laravel-request-logs"
    ],
    "homepage": "https://github.com/dniccum/laravel-request-logs",
    "license": "MIT",
    "authors": [
        {
            "name": "Doug Niccum",
            "email": "dniccumdesign@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "laravel/framework": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.13.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Dniccum\\LaravelRequestLogs\\": "src",
            "Dniccum\\LaravelRequestLogs\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dniccum\\LaravelRequestLogs\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dniccum\\LaravelRequestLogs\\LaravelRequestLogsServiceProvider"
            ],
            "aliases": {
                "LaravelRequestLogs": "Dniccum\\LaravelRequestLogs\\Facades\\LaravelRequestLogs"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
